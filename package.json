{
	"name": "incept-template",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "bun --bun next dev",
		"dev:inngest": "bun --bun inngest-cli dev --queue-workers 4000 --poll-interval 1",
		"build": "bun --bun next build",
		"start": "bun --bun next start",
		"lint": "bun --bun biome lint",
		"format": "bun --bun biome format --write",
		"typecheck": "bun --bun tsc",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:drop": "bun run src/db/scripts/drop.ts"
	},
	"dependencies": {
		"@aws-sdk/client-rds-data": "^3.926.0",
		"@superbuilders/errors": "^3.0.2",
		"@superbuilders/slog": "^4.0.0",
		"@t3-oss/env-nextjs": "^0.13.8",
		"arkregex": "^0.0.2",
		"drizzle-orm": "^0.44.7",
		"he": "^1.2.0",
		"inngest": "^3.45.0",
		"linkedom": "^0.18.12",
		"next": "^16.0.1",
		"openai": "^6.8.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"tar-stream": "^3.1.7",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.4",
		"@tailwindcss/postcss": "^4.1.17",
		"@types/bun": "^1.3.1",
		"@types/he": "^1.2.3",
		"@types/node": "^24.10.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@types/tar-stream": "^3.1.4",
		"babel-plugin-react-compiler": "^1.0.0",
		"drizzle-kit": "^0.31.6",
		"inngest-cli": "^1.13.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3"
	},
	"ct3aMetadata": {
		"initVersion": "7.40.0"
	},
	"trustedDependencies": [
		"inngest-cli"
	]
}
